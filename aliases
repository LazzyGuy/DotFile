# alias file 

alias workspace="cd /mnt/workspace"
alias cls="clear"
alias todo="cat ~/.todorc"
alias pact="pipenv shell"
alias p3="python3"
export PATH="$PATH:/home/suraj/.local/bin"


alias v='NVIM_TUI_ENABLE_TRUE_COLOR=1 nvim'
alias vim='NVIM_TUI_ENABLE_TRUE_COLOR=1 nvim'

export TERM=xterm-256color 

alias color-tmux="export TERM=xterm-256color"
alias tmux="tmux -u"
stty -ixon

alias rm="rm -i"
